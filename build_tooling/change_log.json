{
"categories": [
    {
        "title": "## ⚠️ Breaking Changes",
        "labels": ["api break", "major"]
    },
    {
        "title": "## 🚀 Features",
        "labels": ["enhancement", "minor"]
    },
    {
        "title": "## 🐛 Fixes",
        "labels": ["bug", "patch"]
    }
],
"pr_template": "- ${{TITLE}} (#${{NUMBER}})",
"template": "${{CHANGELOG}}\n\n\n${{UNCATEGORIZED}}",
"ignore_labels": [
    "no-release-notes"
]
}
