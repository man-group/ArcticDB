{
"categories": [
    {
        "title": "## ⚠️ Breaking Changes",
        "labels": ["api break", "major"]
    },
    {
        "title": "## 🚀 Features",
        "labels": ["enhancement", "minor"]
    },
    {
        "title": "## 🐛 Fixes",
        "labels": ["bug", "patch"]
    }
],
"pr_template": "- ${{TITLE}} (#${{NUMBER}})",
"template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>"
}
